@model PoliSchool.Web.Models.StudentListModel

@{
    ViewData["Title"] = "Details";
}

<h3>Detalle del estudiante: @Model.Name</h3>

<div>
  
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
           Código
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class = "col-sm-2">
            Nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
       
        <dt class = "col-sm-2">
            Fecha Inscripción
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EnrollmentDate)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.Id  }) |
    <a asp-action="Index">Volver a la lista</a>
</div>
